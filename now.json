{
  "name": "vue-realworld-example-app",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(js|css|img|fonts|media)/.*",
      "headers": { "cache-control": "max-age=31536000, immutable" }
    },
    { "handle": "filesystem" },
    { "src": ".*", "dest": "/" }
  ],
  "alias": "destroyor.xyz"
}
